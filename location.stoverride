name: Location Services
desc: Change Geo Country Code.

http:
  mitm:
    - "gspe1-ssl.ls.apple.com"
  script:
    - match: ^https?:\/\/gspe1-ssl\.ls\.apple\.com\/pep\/gcc
      name: GeoServices
      type: request
      require-body: false
      timeout: 10

script-providers:
  GeoServices:
    url: https://raw.githubusercontent.com/VirgilClyne/iRingo/main/js/Location.js
    interval: 86400
