name: DNS
desc: Use DNSs based on GeoIP.

dns:
  enable: true
  listen: 0.0.0.0:53
  default-nameserver:
    - 114.114.114.114
    - 8.8.8.8
  enhanced-mode: fake-ip
  fake-ip-range: 198.18.0.1/16
  nameserver:
    - https://i.passcloud.xyz/dns-query
  fallback:
    - https://1.1.1.1/dns-query
    - https://8.8.8.8/dns-query
    - https://dns.adguard.com/dns-query
  fallback-filter:
    geoip: true
    geolp-code: CN
    ipcidr:
      - 240.0.0.04
