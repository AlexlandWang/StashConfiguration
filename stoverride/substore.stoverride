name: Sub-Store
desc: Advanced Subscription Manager.

rules:
- DOMAIN-SUFFIX,vercel.app,$tash

http:
  mitm:
    - sub.store
  script:
    - match: ^https?:\/\/sub\.store
      name: Sub-Store
      type: request
      require-body: true
      timeout: 120
      
script-providers:
  Sub-Store:
    url: https://raw.githubusercontent.com/Peng-YM/Sub-Store/master/backend/sub-store.min.js
    interval: 86400
