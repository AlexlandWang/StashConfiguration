name: iQDNS
desc: Resolve domains with iQDNS.

dns:
  enable: true
  ipv6: true
  listen: 0.0.0.0:53
  default-nameserver:
    - 114.114.114.114
    - 8.8.8.8
  enhanced-mode: fake-ip
  fake-ip-range: 198.18.0.1/16
  nameserver:
    - https://a.passcloud.xyz/dns-query
  fallback:
    - https://worldwide.passcloud.xyz/dns-query
  fallback-filter:
    geoip: true
    geolp-code: CN
    ipcidr:
      - 240.0.0.0/4
