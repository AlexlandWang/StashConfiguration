name: Geography Services
desc: Pass geography detection.

http:
  mitm:
    - "gspe1-ssl.ls.apple.com"
  script:
    - match: ^https?:\/\/gspe\d*-ssl\.ls\.apple\.com(\/pep\/gcc)
      name: GeographyServices
      type: request
      require-body: false
      timeout: 10

script-providers:
  GeographyServices:
    url: https://raw.githubusercontent.com/VirgilClyne/iRingo/main/js/Geo_Services_US.js
    interval: 86400
