name: Geography
desc: Pass geography detection.

http:
  mitm:
    - "gspe1-ssl.ls.apple.com"
  script:
    - match: ^https?:\/\/gspe\d*-ssl\.ls\.apple\.com(\/pep\/gcc)
      name: Geography
      type: request
      require-body: false
      timeout: 10

script-providers:
  Geography:
    url: https://raw.githubusercontent.com/shindgewongxj/WHATSINStash/main/js/geography.js
    interval: 86400
